
#_schedule.blank_bg
  .container
    .row.centered
      %h3.ouishare_stencil
        = t('dynamic_translations.main_schedule.title')
      %br
      %br
      .container
        %p
          = t('dynamic_translations.main_schedule.first_part')
          
          / %br
          / %br
          / %br
          / = t('dynamic_translations.main_schedule.second_part')

    .row.centered
      %a#sched-embed{:href => "http://ouisharefest14.sched.org/"}
      %script{:type => "text/javascript", :src => "http://ouisharefest14.sched.org/js/embed.js"}

.grey_bg#_sharing_fair
  .container
    .row.centered
      %h3.ouishare_stencil
        = t('dynamic_translations.sharing_fair.title')
    %br
    %br
    .col-xs-12.col-md-6.col-lg-6
      
      
      .row.centered
        %p
          = t('dynamic_translations.sharing_fair.text')
      %br
      %br
      
      
      %a{:href => 'http://sched.co/1dhcKMF', :target => '_blank'}
        %button.btn.btn-embossed.btn-primary
          = t('dynamic_translations.tickets.find_out_more')
      &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      %a{:href => 'join#_tickets'}
        %button.btn.btn-embossed.btn-primary
          = t('dynamic_translations.tickets.register_for_free')


    .col-xs-12.col-md-6.col-lg-6      
      %img{:src => '/assets/sharing_fair.png', :alt => 'sharing fair picture', :style => 'width: 560px;'}
      
          

